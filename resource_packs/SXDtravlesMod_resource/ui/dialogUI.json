{
   "btn_next@common.button": {
      "$default_texture": "textures/netease/common/button/default",
      "$hover_texture": "textures/netease/common/button/hover",
      "$label_color": [1, 1, 1],
      "$label_font_scale_factor": 1.0,
      "$label_font_size": "large",
      "$label_layer": 3,
      "$label_offset": [0, 0],
      "$label_text": "继续",
      "$pressed_button_name": "%dialogUI.btnNextClick",
      "$pressed_texture": "textures/netease/common/button/pressed",
      "$texture_layer": 2,
      "anchor_from": "center",
      "anchor_to": "center",
      "bindings": [{
         "binding_collection_name": "",
         "binding_condition": "always_when_visible",
         "binding_type": "collection_details"
      }],
      "controls": [{
            "default@dialogUI.default": {}
         },
         {
            "pressed@dialogUI.pressed": {}
         },
         {
            "hover@dialogUI.hover": {}
         },
         {
            "button_label@dialogUI.button_label": {}
         }
      ],
      "default_control": "default",
      "hover_control": "hover",
      "layer": 1,
      "offset": ["28%", "35%"],
      "pressed_control": "pressed",
      "size": [60, 34],
      "type": "button",
      "visible": true
   },
   "button_label": {
      "color": "$label_color",
      "font_scale_factor": "$label_font_scale_factor",
      "font_size": "$label_font_size",
      "font_type": "smooth",
      "layer": "$label_layer",
      "max_size": ["100%", "100%"],
      "offset": [0, 0],
      "shadow": false,
      "text": "$label_text",
      "text_alignment": "center",
      "type": "label"
   },
   "default": {
      "layer": "$texture_layer",
      "texture": "$default_texture",
      "type": "image"
   },
   "hover": {
      "layer": "$texture_layer",
      "texture": "$hover_texture",
      "type": "image"
   },
   "bg_img": {
      "anchor_from": "center",
      "anchor_to": "center",
      "controls": [{
            "main_text@dialogUI.main_text": {}
         },
         {
            "btn_next@dialogUI.btn_next": {}
         }
      ],
      "layer": 1,
      "offset": [-1.0, -24.0],
      "size": ["100.0%+5.0px", "80.0%+0.0px"],
      "texture": "textures/ui/bg01@3x",
      "type": "image",
      "uv": [0, 0],
      "uv_size": [369, 205],
      "visible": true
   },
   "main": {
      "controls": [{
         "bg_img@dialogUI.bg_img": {}
      }],
      "type": "screen"
   },
   "main_text": {
      "anchor_from": "center",
      "anchor_to": "center",
      "color": [1, 1, 1],
      "font_scale_factor": 1.0,
      "font_size": "normal",
      "font_type": "smooth",
      "layer": 1,
      "offset": [0.0, -5.0],
      "shadow": true,
      "size": [360.0, 100.0],
      "text": "",
      "text_alignment": "left",
      "type": "label",
      "visible": true
   },
   "namespace": "dialogUI",
   "pressed": {
      "layer": "$texture_layer",
      "texture": "$pressed_texture",
      "type": "image"
   }
}